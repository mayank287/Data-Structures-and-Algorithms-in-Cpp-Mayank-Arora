// Tuple in C++ STL
#include <iostream>
#include<tuple>

using namespace std;

int main()
{
 tuple<string,int,string> t = make_tuple("abc",10,"xyz");
 
 cout << get<0>(t) << " " << get<1> (t) << " " << get<2> (t) << endl;
 
 get<0> (t) = "mayank";
 cout << "after Changing    " << get<0> (t);
 

    return 0;
}
